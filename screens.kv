<Login>:
    #Variables
    user:user
    password:password
    bt_forgotpassword:bt_forgotpassword
    cb_rememberme:cb_rememberme

    #Login Screen Background
    MDCard:
        size_hint: None, None
        size: 300, 470
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        
        #SHADDOW
        elevation: 10
        shadow_radius: 30
        shadow_softness: 56
        shadow_softness_size: 5
        shadow_color: (86/255, 197/255, 150/255, .4)

        orientation: 'vertical'

        #Main Container Login Screen
        BoxLayout:
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            orientation: 'vertical'
            spacing: 25
            
            #Header
            MDLabel:
                #ID[   ]
                id: welcome_label
                
                #DESIGN
                text: "Login"
                font_size: 40
                halign: 'center'
                size_hint_y: None
                height: self.texture_size[1]
                padding_y: 15

            # Username
            MDTextField:
                #ID[   ]
                id: user
                #Hint text Username
                hint_text: "Username"
                hint_text_color_normal: (86/255, 197/255, 150/255, 1)
                hint_text_color_focus: (86/255, 197/255, 150/255, 1)

                #Error Text Username
                helper_text: "This field is Required"
                helper_text_mode: "on_error"

                #DESIGN
                text_color_focus: (207/255, 244/255, 210/255, 1)
                line_color_focus: (86/255, 197/255, 150/255, 1)
                
                write_tab:False
                size_hint_x: None
                width: 200
                font_size: 18
                pos_hint: {"center_x": 0.5}
                halign: 'center'

                #EVENTS
                on_text: root.validateText()
                on_text_validate:root.login()
                
            # Password   
            MDTextField:
                #ID[   ]
                id: password

                #Hint text Password
                hint_text: "Password"
                hint_text_color_normal: (86/255, 197/255, 150/255, 1)
                hint_text_color_focus: (86/255, 197/255, 150/255, 1)

                #Error Text Password
                helper_text: "This field is Required"
                helper_text_mode: "on_error"
                
                #DESIGN
                text_color_focus: (207/255, 244/255, 210/255, 1)
                line_color_focus: (86/255, 197/255, 150/255, 1)
                
                write_tab:False
                size_hint_x: None
                width: 200
                font_size: 18
                pos_hint: {"center_x": 0.5}
                password: True
                halign: 'center'
                
                #EVENTS
                on_text: root.validateText()
                on_text_validate:root.login()
                
            #Forgot Password / Remember Me
            BoxLayout:               
                spacing:15
                padding:50,0,50,10
                size_hint_y: None
                height: 25
                #Forgot Password Anchor
                AnchorLayout:
                    size_hint:2,1
                    #ForgotPassword Button
                    MDTextButton:
                        #ID[   ]
                        id: bt_forgotpassword
                        
                        #DESIGN
                        text: "Password?"
                        font_size: 10
                        
                        #EVENTS
                        on_press: root.forgotPassword()
                        color: (86/255, 197/255, 150/255, 1)
                
                #Remember Me Text
                Label:
                #Design
                    text:"      Remember Me"
                    font_size: 10
                #Remember Me Checkbox
                CheckBox:
                    #ID[   ]
                    id:cb_rememberme

                    #Design
                    color:(86/255, 197/255, 150/255, 1)

            # Login Button
            MDRaisedButton:
                #DESIGN
                text: "LOG IN"
                font_size: 12
                pos_hint: {"center_x": 0.5}
                md_bg_color:(86/255, 197/255, 150/255, 1)
                text_color:(0,0,0,1)
                line_color: (86/255, 197/255, 150/255, 1)
                
                #EVENTS
                on_press:
                    root.login()
            
            #Register Button
            MDRaisedButton:
                #Design
                text: "SIGN UP"
                font_size: 12
                pos_hint: {"center_x": 0.5}
                md_bg_color:(86/255, 197/255, 150/255, 1)
                text_color:(0,0,0,1)
                line_color: (86/255, 197/255, 150/255, 1)

                #EVENTS
                on_press:
                    root.register()
            
            #Bottom Spacer
            Widget:
                size_hint_y: None
                height: 10



