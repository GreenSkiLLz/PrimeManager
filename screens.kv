<Login>:

    user:user
    password:password

    MDCard:
        size_hint: None, None
        size: 300, 400
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        elevation: 10
        padding: 25
        spacing: 25
        orientation: 'vertical'

        MDLabel:
            id: welcome_label
            text: "Login"
            font_size: 40
            halign: 'center'
            size_hint_y: None
            height: self.texture_size[1]
            padding_y: 15

        MDTextField:
            id: user
            mode:"round"
            hint_text: "username"
            helper_text: "This field is Required"
            helper_text_mode: "on_error"
            text_color_focus: (207/255, 244/255, 210/255, 1)
            line_color_focus: (86/255, 197/255, 150/255, 1)
            on_text: root.validateText()
            on_text_validate:root.login()
            #icon_right: "account"
            write_tab:False
            size_hint_x: None
            width: 200
            font_size: 18
            pos_hint: {"center_x": 0.5}

        MDTextField:
            id: password
            mode:"round"
            hint_text: "password"
            write_tab:False
            helper_text: "This field is Required"
            helper_text_mode: "on_error"
            text_color_focus: (207/255, 244/255, 210/255, 1)
            line_color_focus: (86/255, 197/255, 150/255, 1)
            on_text: root.validateText()
            on_text_validate:root.login()
            #icon_right: "eye-off"
            size_hint_x: None
            width: 200
            font_size: 18
            pos_hint: {"center_x": 0.5}
            password: True

        MDRoundFlatButton:
            text: "LOG IN"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            text_color:(86/255, 197/255, 150/255, 1)
            line_color: (86/255, 197/255, 150/255, 1)
            on_press:
                root.login()

        MDRoundFlatButton:
            text: "SIGN UP"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            text_color:(86/255, 197/255, 150/255, 1)
            line_color: (86/255, 197/255, 150/255, 1)
            on_press:
                root.register()

        Widget:
            size_hint_y: None
            height: 10



