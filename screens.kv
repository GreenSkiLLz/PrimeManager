<GeneratePasswordPopup_Pop@Popup>

    #Options
    slide_chars:slide_chars
    cb_upperlower:cb_upperlower
    cb_numbers:cb_numbers
    cb_symbols:cb_symbols

    #Labels
    lb_generatedPW:lb_generatedPW
    
    #Main Popup Settings
    title:'Generate new Password, !!Warning!! old password will be lost!'
    title_size:25
    separator_color:(86 / 255, 197 / 255, 150 / 255, 1)
    size_hint:(None, None)
    size:(600, 600)

    #Main GeneratePasswordPopup_Pop Container
    BoxLayout:
        orientation:"vertical"
        padding:(0, 100, 0, 0)
        spacing:10

        #Settings box
        BoxLayout:
            orientation:"vertical"
            spacing:10

            #Option 1: Slider Character Count
            BoxLayout:

                #Option Text
                Label:
                    #Text[   ]
                    text:"Characters"  

                    #DESIGN  
                    size_hint:.2,1
                
                # Current Characters from Slider
                Label:
                    #Param[   ]
                    text:str(slide_chars.value)

                    #DESIGN
                    size_hint:.1,1
                
                # Character Slider
                MDSlider:
                    #ID[   ]
                    id:slide_chars

                    #DESIGN
                    size_hint:.7,1
                    color:(86/255, 197/255, 150/255, 1)
                    hint: True
                    hint_text_color: "white"
                    thumb_color_active: (207/255, 244/255, 210/255, 1)
                    thumb_color_inactive: (86/255, 197/255, 150/255, 1)
                    value: 32
                    step:1
                    min: 8
                    max: 64
            
            #Option 2: Checkbox Capsletters
            BoxLayout:
                
                #Option Text
                Label:
                    #Text[   ]
                    text:"Mix Upper/Lower?"
                    size_hint:.2,1
                
                #Option CHeckbox
                CheckBox:   
                    #ID[   ]
                    id:cb_upperlower

                    #DESIGN
                    active:True
                    size_hint:.1,1
                    color:(86/255, 197/255, 150/255, 1)
                
                #Option Explanation Text
                Label:
                    #Text[   ]
                    text:"( a - z / A - Z)"
                    size_hint:.7,1 
            
            #Option 3: Checkbox Numbers
            BoxLayout:
                
                #Option Text
                Label:
                    #Text[   ]
                    text:"Numbers?"
                    size_hint:.2,1
                
                #Option Checkbox
                CheckBox:
                    #ID[   ]
                    id:cb_numbers

                    #DESIGN
                    active:True
                    size_hint:.1,1
                    color:(86/255, 197/255, 150/255, 1)
                
                #Option Explanation Text
                Label:
                    #Text[   ]
                    text:"( 0 - 9 )"
                    size_hint:.7,1 

            #Option 4: Checkbox Symbols
            BoxLayout:

                #Option Text
                Label:
                    #Text[   ]
                    text:"Symbols?"
                    
                    #DESIGN
                    size_hint:.2,1

                #Option Checkbox    
                CheckBox:
                    #ID[   ]
                    id:cb_symbols

                    #DESIGN
                    active:True
                    size_hint:.1,1
                    color:(86/255, 197/255, 150/255, 1)

                #option Explanation Text
                Label:
                    #Text[   ]  
                    text:"( ! # $ % ( ) * + , - . / : ; = ? @ [ ] ^ _ { | } ~ )"

                    #DESIGN
                    size_hint:.7,1
            
            #Generated Password
            MDTextField:
                #ID [   ]  
                id:lb_generatedPW
                
                #EVENTS
                on_text:root.savePassword(lb_generatedPW)

                #DESIGN
                text:""
                hint_text: "Edit Password here:"
                hint_text_color_focus:(86/255, 197/255, 150/255, 1)
                write_tab:False
                color_mode: 'custom'
                text_color_normal:(86/255, 197/255, 150/255, 1)
                text_color_focus: (207/255, 244/255, 210/255, 1)
                line_color_focus: (86/255, 197/255, 150/255, 1)   
        
        #Button Box
        BoxLayout:
            spacing:140

            #Confirm Generated Password Button
            MDRaisedButton:
                #DESIGN
                md_bg_color:(123 / 255, 228 / 255, 149 / 255, 1)
                pos_hint:{'center_x': .4, 'center_y': .6}
                text:"OK"
                text_color:(0, 0, 0, 1)

                #EVENTS
                on_press:root.returnpassword()

            #Generate new Password Button
            MDRaisedButton:
                #DESIGN
                md_bg_color:(123 / 255, 228 / 255, 149 / 255, 1)
                pos_hint:{'center_x': .4, 'center_y': .6}
                text:"        Generate          "
                text_color:(0, 0, 0, 1)

                #EVENTS
                on_press:root.generatePW(lb_generatedPW,slide_chars,cb_upperlower,cb_numbers,cb_symbols)

            #Cancle Password Generator Popup
            MDRaisedButton:
                #DESIGN
                md_bg_color:(123 / 255, 228 / 255, 149 / 255, 1)
                pos_hint:{'center_x': .4, 'center_y': .6}
                text:"Cancel"
                text_color:(0, 0, 0, 1)

                #EVENTS
                on_press:root.dismiss()
        
